{
    "name": "w2w/laravel-apie",
    "description": "Laravel wrapper for w2w/apie",
    "type": "library",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Pieter Jordaan",
            "email": "pieter_jordaan@hotmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "doctrine/annotations": "^1.7",
        "doctrine/cache": "^1.8",
        "illuminate/container": "^5.6|^6.0",
        "illuminate/contracts": "^5.6|^6.0",
        "illuminate/database": "^5.6|^6.0",
        "illuminate/http": "^5.6|^6.0",
        "illuminate/routing": "^5.6|^6.0",
        "ramsey/uuid": "^3.8",
        "symfony/cache": "^4.4|^5.0",
        "symfony/options-resolver": "^4.4|^5.0",
        "symfony/psr-http-message-bridge": "^1.2",
        "symfony/serializer": "^4.4|^5.0",
        "w2w/apie": "^2.3.0"
    },
    "autoload": {
        "psr-4": {
            "W2w\\Laravel\\Apie\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "W2w\\Laravel\\Apie\\Tests\\": "tests/"
        }
    },
    "archive": {
        "exclude": ["/tests", "/docs"]
    },
    "extra": {
        "laravel": {
            "providers": [
                "W2w\\Laravel\\Apie\\Providers\\ApiResourceServiceProvider"
            ],
            "aliases": {
                "Apie": "W2w\\Laravel\\Apie\\Facades\\Apie"
            }
        }
    },
    "require-dev": {
        "mockery/mockery": "^1.2",
        "nunomaduro/larastan": "^0.4.3",
        "orchestra/testbench": "3.6.*"
    },
    "suggest": {
        "madewithlove/illuminate-psr-cache-bridge": "For caching in Laravel 5.*"
    }
}
