{
    "name": "w2w/laravel-apie",
    "description": "Laravel wrapper for w2w/apie",
    "type": "library",
    "license": "GPL-3.0-or-later",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Pieter Jordaan",
            "email": "pieter.jordaan@way2web.nl"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/container": "^5.6|^6.0",
        "illuminate/contracts": "^5.6|^6.0",
        "illuminate/database": "^5.6|^6.0",
        "illuminate/http": "^5.6|^6.0",
        "illuminate/routing": "^5.6|^6.0",
        "symfony/options-resolver": "^4.0",
        "symfony/serializer": "^4.0",
        "w2w/apie": "^1.0",
        "madewithlove/illuminate-psr-cache-bridge": "^1.0",
        "ramsey/uuid": "^3.8",
        "symfony/psr-http-message-bridge": "^1.2",
        "gbprod/uuid-normalizer": "^1.1",
        "doctrine/cache": "^1.8",
        "doctrine/annotations": "^1.7",
        "symfony/cache": "^4.3"
    },
    "autoload": {
        "psr-4": {
            "W2w\\Laravel\\Apie\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "W2w\\Laravel\\Apie\\Tests\\": "tests/"
        }
    },
    "archive": {
        "exclude": ["/tests", "/docs"]
    },
    "extra": {
        "laravel": {
            "providers": [
                "W2w\\Laravel\\Apie\\Providers\\ApiResourceServiceProvider"
            ],
            "aliases": {
                "ApiResource": "W2w\\Lib\\Apie\\ApiResourceFacade"
            }
        }
    },
    "require-dev": {
        "orchestra/testbench": "3.6.*"
    }
}
